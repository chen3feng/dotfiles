# adb
alias ad='adb devices'


# Emulators with hardware acceleration on Linux.
alias nexus4="emulator -avd AVD_for_Nexus_4_by_Google -scale 0.6 -qemu -m 512 -enable-kvm &"
alias nexus7="emulator -avd AVD_for_Nexus_7_by_Google -scale 0.6 -qemu -m 512 -enable-kvm &"
alias nexus10="emulator -avd AVD_for_Nexus_10_by_Google -scale 0.6 -qemu -m 512 -enable-kvm &"
alias tablet10="emulator -avd AVD_for_10_1in_WXGA_Tablet -scale 0.6 -qemu -m 512 -enable-kvm &"

